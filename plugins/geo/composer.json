{
    "name": "k-box/kbox-plugin-geo",
    "type": "kbox-plugin",
    "description": "Geo K-Box plugin",
    "keywords": ["k-box", "k-link", "geo", "plugin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alessio Vertemati",
            "email": "alessio@oneofftech.xyz"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-zip": "*",
        "oneofftech/geoserver-client-php": "0.2.*",
        "justinrainbow/json-schema": "^5.2",
        "proj4php/proj4php": "^2.0",
        "spinen/laravel-geometry": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "KBox\\Geo\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "kbox": {
            "providers": [
                "KBox\\Geo\\Providers\\GeoServiceProvider"
            ]
        }
    }
}